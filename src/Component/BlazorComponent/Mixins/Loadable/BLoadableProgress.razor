@namespace BlazorComponent
@typeparam TComponent
@inherits ComponentPartBase<TComponent>

@if (Loading is not null && Loading != false)
{
    if (ProgressContent != null)
    {
        @ProgressContent
    }
    else
    {
        <BProgressLinear @attributes="GetAttributes(typeof(BProgressLinear))"></BProgressLinear>
    }
}

