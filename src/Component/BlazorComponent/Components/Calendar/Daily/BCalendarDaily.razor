@namespace BlazorComponent
@inherits BCalendarIntervals

<BCascadingValue Value="this">
    <div class="@CssProvider.GetClass()" style="@CssProvider.GetStyle()">
        @if(!HideHeader)
        {
            <AbstractComponent Metadata="AbstractProvider.GetMetadata(typeof(BCalendarDailyHead<>))"></AbstractComponent>
        }
        <AbstractComponent Metadata="AbstractProvider.GetMetadata(typeof(BCalendarDailyBody<>))"></AbstractComponent>
    </div>
</BCascadingValue>