@namespace BlazorComponent
@typeparam TCalendarDaily
@inherits ComponentAbstractBase<TCalendarDaily>

@for (var i = 0; i < Days.Count; i++)
{
    var index = i;
    var day = Days[i];
    <AbstractComponent Metadata="AbstractProvider.GetMetadata(typeof(BCalendarDailyHeadDay<>))" Day="@day" Index="@index"></AbstractComponent>
}