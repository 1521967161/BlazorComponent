@namespace BlazorComponent
@typeparam TRating
@inherits ComponentAbstractBase<TRating>

@if (ItemContent != null)
{
    @ItemContent(Item)
}
else
{
    <AbstractComponent @key="ItemIndex" Metadata="AbstractProvider.GetMetadata<BButton>(ItemIndex)">
        <AbstractComponent Metadata="@(AbstractProvider.GetMetadata<BIcon>())">
            @GetIconName(Item)
        </AbstractComponent>
    </AbstractComponent>
}