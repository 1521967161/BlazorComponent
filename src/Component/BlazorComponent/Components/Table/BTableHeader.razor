@namespace BlazorComponent
@inherits BDomComponentBase

<thead class="@CssProvider.GetClass()" id="@Id" @ref="Ref">
    <tr>
        @if (Headers != null)
        {
            @foreach (var header in Headers)
            {
                <th class="@CssProvider.GetClass("th")">@header</th>
            }
        }
    </tr>
</thead>
