@namespace BlazorComponent
@inherits BDomComponentBase

<CascadingValue Value="this">
    <CascadingValue Value="true" Name="IsInList">
        <Element Tag="@Tag" Class="@CssProvider.GetClass()" Style="@CssProvider.GetStyle()" ReferenceCaptureAction="@((_) => Ref = _)" @attributes="Attributes">
            @ChildContent
        </Element>
    </CascadingValue>
</CascadingValue>