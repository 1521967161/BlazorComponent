@namespace BlazorComponent
@inherits BDomComponentBase


<BPicker Class="@CssProvider.GetClass()" @attributes="GetAttributes(typeof(BPicker))">
    <TitleContent>
        @if (!NoTitle)
        {
            <BTimePickerTitle @attributes="GetAttributes(typeof(BTimePickerTitle))"></BTimePickerTitle>
        }
    </TitleContent>
    <ChildContent>
        @RenderPart(typeof(BTimePickerPickerBody<>))
    </ChildContent>
    <ActionsContent>
        @ChildContent
    </ActionsContent>
</BPicker>

