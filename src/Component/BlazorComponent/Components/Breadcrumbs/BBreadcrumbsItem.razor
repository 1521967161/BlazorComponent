@namespace BlazorComponent
@inherits BDomComponentBase

<CascadingValue Value="this" IsFixed>

    <Element Tag="@Tag" class="@CssProvider.GetClass(nameof(BBreadcrumbsItem))" style="@CssProvider.GetStyle(nameof(BBreadcrumbsItem))">
        @{
            var (tag, props) = ChildProps;
        }
        <Element Tag="@tag" ExtraAttributes=props>
            @ChildContent
        </Element>

    </Element>

</CascadingValue>