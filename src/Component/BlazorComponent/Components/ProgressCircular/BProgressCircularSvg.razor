@namespace BlazorComponent
@typeparam TProgressCircular
@inherits ComponentAbstractBase<TProgressCircular>

<svg style="@CssProvider.GetStyle("svg")" @attributes="SvgAttributes">
    @if(!Indeterminate)
    {
        <AbstractComponent Name="underlay" Offset="0" Metadata="AbstractProvider.GetMetadata(typeof(BProgressCircularCircle<>))"></AbstractComponent>
    }
    <AbstractComponent Name="overlay" Offset="@StrokeDashOffset" Metadata="AbstractProvider.GetMetadata(typeof(BProgressCircularCircle<>))"></AbstractComponent>
</svg>