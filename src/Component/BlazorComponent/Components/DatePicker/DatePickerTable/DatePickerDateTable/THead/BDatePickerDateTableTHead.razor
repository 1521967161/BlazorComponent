@namespace BlazorComponent
@typeparam TDatePickerDateTable
@inherits ComponentPartBase<TDatePickerDateTable>

<thead>
    @{
        //REVIEW: How about remove this to BDatePickerDateTableTR<>
        RenderFragment childContent = builder =>
                {
                    var sequence = 0;
                    if (ShowWeek)
                    {
                        builder.AddContent(sequence++,@<th></th>);
                    }
                    foreach (var day in WeekDays)
                    {
                        builder.AddContent(sequence++,@<th>@day</th>);
                    }
                };
    }
    @RenderPart(typeof(BDatePickerDateTableTR<>),childContent)
</thead>

