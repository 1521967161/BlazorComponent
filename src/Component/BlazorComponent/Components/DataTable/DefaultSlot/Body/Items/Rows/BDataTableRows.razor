@namespace BlazorComponent
@typeparam TItem
@typeparam TDataTable
@inherits ComponentAbstractBase<TDataTable>

@if (ItemContent != null)
{
    <AbstractComponent Items="@Items" Metadata="AbstractProvider.GetMetadata(typeof(BDataTableScopedRows<,>))"></AbstractComponent>
}
else
{
    <AbstractComponent Items="@Items" Metadata="AbstractProvider.GetMetadata(typeof(BDataTableDefaultRows<,>))"></AbstractComponent>
}

